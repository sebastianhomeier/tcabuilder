{
  "name": "spooner-web/tcabuilder",
  "description": "Utility to easily maintain and create your TCA forms",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Thomas LÃ¶ffler",
      "role": "Developer",
      "email": "loeffler@spooner-web.de",
      "homepage": "https://spooner-web.de"
    }
  ],
  "support": {
    "issues": "https://git.spooner.io/spooner-web/tcabuilder/issues"
  },
  "funding": [
    {
      "type": "other",
      "url": "https://paypal.me/Tomalo"
    }
  ],
  "require": {
    "php": ">= 7.2, < 8.0",
    "typo3/cms-backend": "^v8.7 || ^v9.5 || ^v10.4 || ^v11.4",
    "typo3/cms-core": "^v8.7 || ^v9.5 || ^v10.4 || ^v11.4"
  },
  "require-dev": {
    "nimut/testing-framework": "^6.0",
    "rector/rector": "^v0.12"
  },
  "autoload": {
    "psr-4": {
      "SpoonerWeb\\TcaBuilder\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SpoonerWeb\\TcaBuilder\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "test:unit": "phpunit -c ./.gitlab-ci/phpunit.xml",
    "test:rector": "rector process -c ./.gitlab-ci/rector.php Classes/ -n",
    "run:rector": "rector process -c ./.gitlab-ci/rector.php Classes/"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "tcabuilder",
      "app-dir": ".Build",
      "web-dir": ".Build/public"
    }
  }
}
