{
  "name": "spooner-web/tcabuilder",
  "description": "Utility to easily maintain and create your TCA forms",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Thomas LÃ¶ffler",
      "role": "Developer",
      "email": "loeffler@spooner-web.de",
      "homepage": "https://spooner-web.de"
    }
  ],
  "support": {
    "issues": "https://git.spooner.io/spooner-web/tcabuilder/issues"
  },
  "funding": [
    {
      "type": "other",
      "url": "https://paypal.me/Tomalo"
    }
  ],
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "require": {
    "php": ">= 7.2, < 8.1",
    "typo3/cms-backend": "^v8.7 || ^v9.5 || ^v10.4 || ^v11.0",
    "typo3/cms-core": "^v8.7 || ^v9.5 || ^v10.4 || ^v11.0"
  },
  "require-dev": {
    "typo3/testing-framework": "^6.6",
    "rector/rector": "^v0.7"
  },
  "autoload": {
    "psr-4": {
      "SpoonerWeb\\TcaBuilder\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SpoonerWeb\\TcaBuilder\\Tests\\": "Tests"
    }
  },
  "scripts": {
    "test:unit": "phpunit -c ./.Build/phpunit.xml",
    "post-autoload-dump": [
      "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "extension-key": "tcabuilder"
    }
  }
}
